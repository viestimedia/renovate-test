{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["config:recommended"],
  "hostRules": [
    {
      "matchHost": "npm.pkg.github.com/",
      "hostType": "npm",
      "token": "{{ secrets.GITHUB_TOKEN }}"
    }
  ],
  "env": {
    "GITHUB_TOKEN": "{{ secrets.GITHUB_TOKEN }}"
  }
}
